            <name>shovill</name>
            <owner>iuc</owner>
            <url>https://toolshed.g2.bx.psu.edu</url>
            <revision>d9f6a00b6db7</revision>
        </repository>
        <repository>
            <name>prokka</name>
            <owner>crs4</owner>
            <url>https://toolshed.g2.bx.psu.edu</url>
            <revision>3a0bd09a41dd</revision>
        </repository>
        <repository>
            <name>quast</name>
            <owner>iuc</owner>
            <url>https://toolshed.g2.bx.psu.edu</url>
            <revision>ebb0dcdb621a</revision>
        </repository>
    </toolRepositories>
